  "main": "index.js",
  "scripts": {
    "clone": "git clone https://github.com/risd/webhook-generate.git generator",
    "zip": "zip-dir generator base-generate.zip",
    "mv": "mkdir -p upload && mv base-generate.zip upload",
    "upload": "node upload.js --debug",
